<block>
  <name>EOP Clearance Probe</name>
  <key>wifi_zigbee_eop_clearance_probe</key>
  <category>[WIFI_ZIGBEE]</category>
  <import>import wifi_zigbee</import>
  <make>wifi_zigbee.eop_clearance_probe($debug, $threshold)</make>
  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>
    
    <option>
	<name>Enable</name>
	<key>True</key> 
    </option>

    <option>
	<name>Disable</name>
	<key>False</key> 
    </option>
	
  </param>
    <param>
    <name>Threshold</name>
    <key>threshold</key>
    <value>5</value>
    <type>int</type>
  </param>
  <sink>
    <name>in_real</name>
    <type>float</type>
    <nports>1</nports>
  </sink>
  <sink>
    <name>in_img</name>
    <type>float</type>
    <nports>1</nports>
  </sink>
  <source>
    <name>out_sm_real</name>
    <type>float</type>
    <nports>1</nports>
  </source>
  <source>
    <name>out_sm_img</name>
    <type>float</type>
    <nports>1</nports>
  </source>
  <source>
    <name>out_long_real</name>
    <type>float</type>
    <nports>1</nports>
  </source>
  <source>
    <name>out_long_img</name>
    <type>float</type>
    <nports>1</nports>
  </source>
</block>
