<block>
  <name>ZigBee Reeler (Medium)</name>
  <key>wifi_zigbee_zigbee_reeler_medium</key>
  <category>[WIFI_ZIGBEE]</category>
  <import>import wifi_zigbee</import>
  <make>wifi_zigbee.zigbee_reeler_medium($debug)</make>
  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>

	<option>
		<name>Enable</name>
		<key>True</key>
	</option>
	<option>
		<name>Disable</name>
		<key>False</key>
	</option>
  </param>
  <sink>
    <name>in_real</name>
    <type>float</type>
    <nports>1</nports>
  </sink>
  <sink>
    <name>in_img</name>
    <type>float</type>
    <nports>1</nports>
  </sink>
  <source>
    <name>out_zigbee</name>
    <type>complex</type>
    <nports>1</nports>
  </source>
  <source>
    <name>out_wifi</name>
    <type>complex</type>
    <nports>1</nports>
  </source>
</block>
