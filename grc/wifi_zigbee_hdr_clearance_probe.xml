<block>
  <name>Hdr Clearance Probe</name>
  <key>wifi_zigbee_hdr_clearance_probe</key>
  <category>[WIFI_ZIGBEE]</category>
  <import>import wifi_zigbee</import>
  <make>wifi_zigbee.hdr_clearance_probe($debug, $pwr_threshold, $sc_threshold)</make>
  <param>
    <name>Debug</name>
    <key>debug</key>
    <value>False</value>
    <type>bool</type>

	<option>
		<name>Enable</name>
		<key>True</key>
	</option>	
	<option>
		<name>Disable</name>
		<key>False</key>
	</option>	


  </param>
  <param>
    <name>Power</name>
    <key>pwr_threshold</key>
    <type>int</type>
  </param>
  <param>
    <name>Subcarrier</name>
    <key>sc_threshold</key>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <nports>1</nports>
  </sink>
  <source>
    <name>out_s_real</name>
    <type>float</type>
    <nports>1</nports>
  </source>
  <source>
    <name>out_s_img</name>
    <type>float</type>
    <nports>1</nports>
  </source>
  <source>
    <name>out_m_real</name>
    <type>float</type>
    <nports>1</nports>
  </source>
  <source>
    <name>out_m_img</name>
    <type>float</type>
    <nports>1</nports>
  </source>
  <source>
    <name>out_l_real</name>
    <type>float</type>
    <nports>1</nports>
  </source>
  <source>
    <name>out_l_img</name>
    <type>float</type>
    <nports>1</nports>
  </source>
</block>
